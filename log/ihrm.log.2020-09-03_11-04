<script>
var baseUrl = $("script[src]").get(0).src.replace("assets/plugins/jquery/jquery-1.9.1.min.js", "");

var loginButton = $('.btn-block'),
        loginAccount = $("#loginAccount"),
        password = $("#password"),
        firstEnter = 1,
        url,
        requestUrl = localStorage.getItem("firstUrl");

    loginButton.removeAttr('disabled');

    url = requestUrl ? requestUrl : baseUrl + 'portal.jsp';
    //url = requestUrl ? requestUrl : baseUrl + "index2.html";

    localStorage.setItem("firstUrl", '');

    $("#loginAccount, #password").keyup(function (e) {
        e.stopPropagation();
        if (event.keyCode === 13) {
            loginButton.click();
        } else {
            setTimeout(function () {
                loginButton.removeClass("btn-warning btn-danger").text("ç«å³ç»å¥");
            }, 0)
        }
    });
    var tijiaozhong = 0;
    loginButton.on("click", function (e) {
        if(tijiaozhong == 1){
            alertInfo("è¯·å¿éå¤ç¹å»");
            return;
        }
        tijiaozhong=1;
        e.preventDefault();
        var t = $(this);
        t.text("ç»å½ä¸­...");
        var login_data = $("#loginForm").serialize();
        if (loginAccount.val() === "") {
            t.text('è¯·è¾å¥ç¨æ·å!').addClass("btn-warning");
            loginAccount.focus();
            tijiaozhong = 0;
            return;
        }
        if (password.val() === "") {
            if (firstEnter) {
                password.focus();
                firstEnter = 0;
                setTimeout(function () {
                    t.click();
                }, 50);
            } else {
                t.text('è¯·è¾å¥å¯ç !').addClass("btn-warning");
                password.focus();
            }
            tijiaozhong = 0;
            return;
        }
        $.ajax({
            url: "login/login",
            type: "post",
            data: login_data,
            dataType: 'json',
            success: function (data) {
                if (data.returnCode === "0000") {
                    sessionStorage.setItem('csrInfo', JSON.stringify(data.data));
                    password.blur();
                    t.text('ç»å½æå, é¡µé¢è·³è½¬ä¸­...').addClass("btn-succezss");
                    if(data.data.mhMenu==="true"){
                        setTimeout(function(){
                            localStorage.setItem("isIndex",false);
                            location.href = url;
                        }, 100);
                    }else if(data.data.mhMenu==="callCenter"){
                        setTimeout(function(){
                            localStorage.setItem("isIndex",true);
                            location.href = '../enesys-callcenter/callCenter-index.jsp';
                        }, 100);
                    }else {
                        setTimeout(function(){
                            localStorage.setItem("isIndex",true);
                            location.href = 'index.jsp';
                        }, 100);
                    }

                } else if (data.returnCode === "4003") {
                    t.text('å¯ç è¿æï¼').addClass("btn-danger");
                    setTimeout(function(){
                        location.href = 'loginEditPwd.html';
                    },1500)
                } else {
                    tijiaozhong = 0;
                    t.text(data.returnMessage).addClass("btn-warning");
                }
            },
            error: function (jqXHR) {
                tijiaozhong = 0;
                t.text('ç»å½å¤±è´¥, è¯·éè¯!').addClass("btn-danger");
                console.error(jqXHR);
            }
        });
    });

    var getDepts = function(){
        var domainName  = document.location.protocol  + "//" + document.location.host  ;

        var login_data = {}
        login_data.domainName = domainName;
        login_data.loginAccount = loginAccount.val();
        if(login_data.loginAccount == ""){
            return;
        }
        $.ajax({
            url: "login/departmentList",
            type: "post",
            data: login_data,
            dataType: 'json',
            success: function (data) {
                console.log(data)
                if (data.returnCode === "0000") {
                    $("#deptId").removeClass("hidden");
                    deptSelectDataLoad(data.data);
                } else if (data.returnCode === "1001") {
                    $("#deptId").find("option").remove();
                    $("#deptId").addClass("hidden");
                } else {
                    loginButton.text(data.returnMessage).addClass("btn-warning");
                }
            },
            error: function (jqXHR) {
                loginButton.text('ç»å½å¤±è´¥, è¯·éè¯!').addClass("btn-danger");
            }
        });
        return

    }

    function getBeianInfo(){
        var domainName  = document.location.protocol  + "//" + document.location.host  ;
        var login_data = {}
        login_data.domainName = domainName;
        $.ajax({
            url: "login/beian",
            type: "post",
            data: login_data,
            dataType: 'json',
            success: function (data) {
                if (data.returnCode === "0000") {
                    console.log($(".link-record > a"))
                    $(".link-record > a").text(data.data.no);
                    $(".link-record > a").attr("href",data.data.url);
                } else {

                }
            },
            error: function (jqXHR) {
                loginButton.text('ç»å½å¤±è´¥, è¯·éè¯!').addClass("btn-danger");
            }
        });
    }

    $(document).ready(function() {
        getBeianInfo()
        $("#loginAccount").on("blur",getDepts)
    });

    function deptSelectDataLoad(depts) {
        var t = $("#deptId");
        if(depts && depts.length>0){
            t.find("option").remove();
            t.append('<option value="">' + (t.data("placeholder") || t.attr("placeholder") || "") + '</option>');
            $.each(depts, function (key, val) {
                var i18_CodeDes = val.deptName;
                t.append('<option value="' +val.deptId+ '">' + i18_CodeDes + '</option>');
            });

            t.prev(".s-loading").fadeOut(100, function () {
                t.prev(".s-loading").remove();
            });
        }
    }
</script>
